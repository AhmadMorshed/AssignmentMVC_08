@model ApplicationUser

<h1>Details User</h1>
<br />
<br />
<div class="row">
    <div class="col-12">
            <div class="from-group">

                <label asp-for="@Model.UserName" class="col-from-label"></label>
                <input asp-for="@Model.UserName" class="from-control" readonly />
               
            </div>
            <br />
        <div class="from-group">

            <label asp-for="@Model.Email" class="col-from-label"></label>
            <input asp-for="@Model.Email" class="from-control" readonly />

        </div>

            <a class="btn btn-success" asp-action="Update" asp-route-id="@Model.Id">Update</a>
            <a class="btn btn-light" asp-action="Index">Back To List</a>
        

    </div>
</div>